         url('{{ 'volksmedita-webfont.woff' | asset_url }}') format('woff'),
         url('{{ 'volksmedita-webfont.ttf' | asset_url }}') format('truetype'),
         url('{{ 'volksmedita-webfont.svg' | asset_url }}#volkswagen-mediumitaregular') format('svg');
