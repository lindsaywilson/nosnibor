
@font-face {
  font-family: 'apple';
  src: url('{{ 'Apple2.eot' | asset_url }}');
  src: local('☺'),
    url('{{ 'Apple2.eot' | asset_url }}?#iefix') format('embedded-opentype'),
    url('{{ 'Apple2.woff' | asset_url }}') format('woff'),
    url('{{ 'Apple2.ttf' | asset_url }}') format('truetype');
    font-weight: normal;
    font-style: normal;
  }

@font-face {
    font-family: 'dado';
    src: url({{ 'Dado.eot' | asset_url }});
    src: local('☺'), 
		 url('{{ 'Dado.eot' | asset_url }}?#iefix') format('embedded-opentype'),
         url('{{ 'Dado.woff' | asset_url }}') format('woff'),
         url('{{ 'Dado.ttf' | asset_url }}') format('truetype');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'volkswagen';
    src: url('{{ 'volksmed-webfont.eot' | asset_url }}');
    src: url('{{ 'volksmed-webfont.eot' | asset_url }}?#iefix') format('embedded-opentype'),
         url('{{ 'volksmed-webfont.woff' | asset_url }}') format('woff'),
         url('{{ 'volksmed-webfont.ttf' | asset_url }}') format('truetype'),
         url('{{ 'volksmed-webfont.svg' | asset_url }}#volkswagen-mediumregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'volkswagen-italic';
    src: url('{{ 'volksmedita-webfont.eot' | asset_url }}');
