#container { margin:0 auto; }
#container { background-color: transparent !important; }




#buttons input, input[type="submit"]{ display:inline-block; border:1px solid #871d0a; margin:0; padding:5px; color:#871d0a; font-weight:normal; font-size:8px;  text-transform:uppercase; background:#ff952b; width:auto; line-height:1; height:auto; font-family:'apple'; letter-spacing:-0.1em; -webkit-font-smoothing: antialiased;  }
#buttons input:hover, input[type="submit"]:hover { background-color:#601307; color:#ff952b; text-decoration:none }

